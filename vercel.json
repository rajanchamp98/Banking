{
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src":"build/**",
      "use":"@vercel/static"
    }
  ],
  "routes": [
    
    {
      "src":"/users/(.*)",
      "dest":"index.js"
    },
    {
      "src": "/register/(.*)",
      "dest": "index.js"
    },
    {
      "src":"/",
      "dest":"build/index.html"
    },
    {
      "src":"/login",
      "dest":"build/index.html"
    },
   
    {
      "src":"/(.+)",
      "dest":"build/index.html"
    }
  ]
 
  
}