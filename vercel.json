{
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src":"build/**",
      "use":"@vercel/static"
    }
  ],
  "routes": [
   
    {
      "src":"/",
      "dest":"index.html"
    },
    {
      "src":"/login",
      "dest":"build/index.html"
    },
    {
      "src":"/register",
      "dest":"build/index.html"
    }
  ]
 
  
}